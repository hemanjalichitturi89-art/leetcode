class Solution:
    def fib(self, n: int) -> int:
        memo={}
        return self.fib_memo(n,memo)
    def fib_memo(self,n:int,memo:dict)->int:
        if n in memo:
           return memo[n]
        if n<=1:
            return n
        memo[n]=self.fib_memo(n-1,memo)+self.fib_memo(n-2,memo)
        return memo[n]
        
